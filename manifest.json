{
    "manifest_version": 2,
    "name": "VoiceCalls",
    "version": "1.0",
    "description": "Use your voice to do stuff in your browser",
    "icons": {
      "48": "icons/voicecalls-48.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["main.js"]
      }
    ]
}